{
  "name": "next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "format": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "hygen": "hygen"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "npm run format"
    ]
  },
  "dependencies": {
    "@svgr/webpack": "5.5.0",
    "@testing-library/react": "12.1.2",
    "@types/jest": "27.0.2",
    "@types/node": "16.10.3",
    "@types/react": "17.0.27",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "clsx": "1.1.1",
    "eslint-config-prettier": "8.3.0",
    "jest": "27.2.5",
    "next": "11.1.2",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "1.42.1",
    "ts-jest": "27.0.5",
    "typescript": "4.4.3"
  },
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@storybook/addon-actions": "6.3.10",
    "@storybook/addon-essentials": "6.4.13",
    "@storybook/addon-links": "6.3.10",
    "@storybook/react": "6.4.13",
    "@testing-library/react": "12.1.2",
    "babel-loader": "8.2.2",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "husky": "7.0.2",
    "hygen": "6.1.0",
    "lint-staged": "11.2.1"
  }
}
